   "source": [
    "num=int(input(\"Enter a thre digit number:\"))\n",
    "# to get first number used floor division operator and divide by 100\n",
    "a=num//100 \n",
    "# divide by 10 and last digit and remainder by 10 \n",
    "b=num//10%10\n",
    "#Get remainder by 10 \n",
    "c=num%10\n",
    "sum=a+b+c\n",
    "print(\"Sum of digits:\",sum)"
   ]
